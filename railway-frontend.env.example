# Railway Frontend Service 환경 변수 예제
# Railway 콘솔에서 각 환경 변수를 설정하세요

# Rails 환경
RAILS_ENV=production

# Rails 암호화 키 (frontend/config/master.key 파일 내용)
RAILS_MASTER_KEY=your_frontend_master_key_here

# Backend API URL (Backend 서비스 도메인)
BACKEND_API_URL=https://your-backend-service.railway.app/api/v1

# Railway 호스트 설정
RAILWAY_STATIC_URL=${RAILWAY_STATIC_URL}
RAILWAY_PUBLIC_DOMAIN=${RAILWAY_PUBLIC_DOMAIN}

# Rails 스레드 설정
RAILS_MAX_THREADS=5